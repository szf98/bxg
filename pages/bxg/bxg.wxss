.nav{
    width: 750rpx;
    height: 150px;
}
.icon {
    width: 100rpx;
    height: 100rpx;
  }
  .icon-list {
    height: 240rpx; 
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx;
  }
  .icon-list view {
    display: flex;
    flex-direction: column;
  }
  /* 列表 */
  .list{
      margin-top: 20px;
      background: #fff;
  }
  .list-nav{
    display: flex;
    justify-content: space-between;
    padding: 0 15rpx;
    margin-bottom: 15px;
  }
  .list-list{
    display: flex;
    margin-top: 20px;
    /* margin-bottom: 10px; */
  }
  .list-left{
    width: 330rpx;
    height: 186rpx;
    margin-right: 10px;
    margin-left: 10px;
  }
  .img{
    border-radius: 8px;
    width: 100%;
    height: 100%;
  }
  .list-right{
    width: 330rpx;
    height: 186rpx;
    font-size: 12px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
  }
  .list-right-bottom{
      display:flex;
      justify-content: space-between;
  }
  .list-money{
      color:red;
  }